<h3>message新規作成</h3>

<%= form_with(model: @message, local: true) do |f| %>
  <%= f.label :title, 'Title' %>
  <%= f.text_field :title %>
  <%= f.label :content, 'Message' %>
  <%= f.text_field :content %>

  <%= f.submit '投稿' %>
<% end %>

<%= link_to 'トップページに戻る', root_path %>
<%= link_to 'Mypageに戻る', mypage_users_path %>